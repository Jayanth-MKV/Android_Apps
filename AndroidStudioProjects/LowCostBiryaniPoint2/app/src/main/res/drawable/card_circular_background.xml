    <item>
        <shape
            android:innerRadius="0dp"
            android:shape="ring"
            android:thicknessRatio="1.9"
            android:useLevel="false" >
            <solid android:color="@android:color/transparent" />

            <stroke
                android:width="10dp"
                android:color="@android:color/white" />
        </shape>


    </item>


</layer-list>
